            .ORG    $8000

MAIN:
            JSR     TURNON
            JSR     DELAY
            JSR     TURNOFF
            JSR     DELAY
            JMP     MAIN

TURNON:
            LDA     #$FF
            STA     $2000
            RTS

DELAY:               ;200 = 0x00C8
;set DELAY_DATA_LSB
            LDA     #$C8
            STA     $5001
; set DELAY_DATA_MSB
            LDA     #$00
            STA     $5002
; set DELAY_DATA_CTRL
            LDA     #$01
            STA     $5000
            RTS

TURNOFF:
            LDA     #$00
            STA     $2000
            RTS

