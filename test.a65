; a simple simple test 6502 asm
; NOTE: the program should start at 0x3000 for avr6502
            .ORG    $3000

START:
            LDA     #$01
            STA     $0200
            LDA     #$05
            STA     $0201
            LDA     #$08
            STA     $0202
            JMP     START ; infinite loop
